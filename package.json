{
    "name": "ceva24.dev",
    "private": true,
    "version": "1.0.0",
    "description": "Static website built with TypeScript and Gatsby",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ceva24/ceva24.dev"
    },
    "keywords": [
        "gatsby",
        "website"
    ],
    "scripts": {
        "prepare": "husky install",
        "develop": "gatsby develop",
        "start": "gatsby develop",
        "build": "gatsby build",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "e2e": "cypress run",
        "e2e:visual": "percy exec -- cypress run",
        "format": "prettier --write .",
        "check-format": "prettier --check .",
        "lint": "xo",
        "check-types:gatsby": "tsc",
        "check-types:cypress": "tsc --project cypress/tsconfig.json",
        "check-types": "npm run check-types:gatsby && npm run check-types:cypress",
        "check": "npm run check-format && npm run lint && npm run check-types"
    },
    "dependencies": {
        "@emotion/react": "^11.4.0",
        "@emotion/styled": "^11.3.0",
        "@fortawesome/fontawesome-svg-core": "^1.2.35",
        "@fortawesome/free-brands-svg-icons": "^5.15.3",
        "@fortawesome/free-solid-svg-icons": "^5.15.3",
        "@fortawesome/react-fontawesome": "^0.1.14",
        "gatsby": "^3.7.2",
        "gatsby-plugin-emotion": "^6.7.1",
        "gatsby-plugin-google-analytics": "^3.7.1",
        "gatsby-plugin-image": "^1.8.0",
        "gatsby-plugin-manifest": "^3.7.1",
        "gatsby-plugin-offline": "^4.7.1",
        "gatsby-plugin-react-helmet": "^4.7.1",
        "gatsby-plugin-sharp": "^3.8.0",
        "gatsby-plugin-typography": "^3.7.1",
        "gatsby-remark-highlights": "^1.3.4",
        "gatsby-remark-images": "^5.5.0",
        "gatsby-source-filesystem": "^3.7.1",
        "gatsby-transformer-remark": "^4.4.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0",
        "react-typography": "^0.16.20",
        "typeface-merriweather": "^1.1.13",
        "typeface-source-sans-pro": "^1.1.13",
        "typography": "^0.16.21",
        "typography-theme-us-web-design-standards": "^0.16.19"
    },
    "devDependencies": {
        "@percy/cli": "^1.0.0-beta.55",
        "@percy/cypress": "^3.1.0",
        "@types/react-helmet": "^6.1.1",
        "@types/typography": "^0.16.3",
        "cypress": "^7.5.0",
        "eslint-config-xo-react": "^0.25.0",
        "eslint-plugin-cypress": "^2.11.3",
        "husky": "^6.0.0",
        "xo": "^0.40.3"
    }
}
