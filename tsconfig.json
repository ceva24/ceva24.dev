{
    "compilerOptions": {
        "module": "commonjs",
        "target": "esnext",
        "jsx": "preserve",
        "lib": ["dom", "es2015", "es2017"],
        "types": ["jest"],
        "strict": true,
        "noEmit": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "removeComments": false,
        "preserveConstEnums": true
    },
    "exclude": ["node_modules", "cypress", "cypress.config.ts"]
}
