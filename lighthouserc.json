{
    "ci": {
        "collect": {
            "url": ["http://localhost:9000/"],
            "startServerCommand": "npm run serve",
            "startServerReadyPattern": "in the browser"
        },
        "assert": {
            "assertions": {
                "categories:performance": ["error", { "minScore": 1 }]
            }
        },
        "upload": {
            "target": "temporary-public-storage"
        }
    }
}
