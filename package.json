{
    "name": "ceva24.dev",
    "private": true,
    "version": "1.0.0",
    "description": "Static website built with TypeScript and Gatsby",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ceva24/ceva24.dev"
    },
    "keywords": [
        "gatsby",
        "website",
        "tailwindcss"
    ],
    "scripts": {
        "prepare": "husky install",
        "develop": "gatsby develop",
        "start": "gatsby develop",
        "build": "gatsby build",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "test": "jest --coverage",
        "e2e": "cypress run",
        "format": "prettier --write .",
        "check-format": "prettier --check .",
        "lint": "xo",
        "check-types:gatsby": "tsc",
        "check-types:cypress": "tsc --project cypress/tsconfig.json",
        "check-types": "npm run check-types:gatsby && npm run check-types:cypress",
        "check": "npm run check-format && npm run lint && npm run check-types && npm run test",
        "fc": "npm run format && npm run check",
        "update": "ncu -u"
    },
    "dependencies": {
        "@fontsource/montserrat": "^4.5.1",
        "@fortawesome/fontawesome-svg-core": "^1.2.36",
        "@fortawesome/free-brands-svg-icons": "^5.15.4",
        "@fortawesome/free-solid-svg-icons": "^5.15.4",
        "@fortawesome/react-fontawesome": "^0.1.16",
        "gatsby": "^4.1.4",
        "gatsby-plugin-google-analytics": "^4.1.0",
        "gatsby-plugin-image": "^2.1.2",
        "gatsby-plugin-manifest": "^4.1.3",
        "gatsby-plugin-offline": "^5.1.3",
        "gatsby-plugin-postcss": "^5.1.0",
        "gatsby-plugin-react-helmet": "^5.1.0",
        "gatsby-plugin-sharp": "^4.1.3",
        "gatsby-remark-highlights": "^1.3.4",
        "gatsby-remark-images": "^6.1.3",
        "gatsby-source-filesystem": "^4.1.2",
        "gatsby-transformer-remark": "^5.1.3",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0"
    },
    "devDependencies": {
        "@percy/cli": "^1.0.0-beta.70",
        "@percy/cypress": "^3.1.1",
        "@reach/router": "^1.3.4",
        "@testing-library/cypress": "^8.0.1",
        "@testing-library/react": "^12.1.2",
        "@types/jest": "^27.0.2",
        "@types/react-helmet": "^6.1.4",
        "autoprefixer": "^10.4.0",
        "cypress": "^9.0.0",
        "eslint-config-xo-react": "^0.25.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-jsx-a11y": "^6.5.1",
        "husky": "^7.0.4",
        "jest": "^27.3.1",
        "postcss": "^8.3.11",
        "tailwindcss": "^2.2.19",
        "xo": "^0.46.4"
    }
}
