{
    "name": "ceva24.dev",
    "private": true,
    "version": "1.0.0",
    "description": "Static website built with TypeScript and Gatsby",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ceva24/ceva24.dev"
    },
    "keywords": [
        "gatsby",
        "website",
        "tailwindcss"
    ],
    "scripts": {
        "prepare": "husky install",
        "develop": "gatsby develop",
        "start": "gatsby develop",
        "build": "gatsby build",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "test": "jest --coverage",
        "e2e": "cypress run",
        "perf": "lhci autorun",
        "format": "prettier --write .",
        "check-format": "prettier --check .",
        "lint": "xo",
        "check-types:gatsby": "tsc",
        "check-types:cypress": "tsc --project cypress/tsconfig.json",
        "check-types": "npm run check-types:gatsby && npm run check-types:cypress",
        "check": "npm run check-format && npm run lint && npm run check-types && npm run test",
        "fc": "npm run format && npm run check",
        "update": "npx npm-check-updates -u"
    },
    "dependencies": {
        "@fontsource/montserrat": "^4.5.14",
        "@fortawesome/fontawesome-svg-core": "^6.3.0",
        "@fortawesome/free-brands-svg-icons": "^6.3.0",
        "@fortawesome/free-solid-svg-icons": "^6.3.0",
        "@fortawesome/react-fontawesome": "^0.2.0",
        "@tsconfig/cypress": "^1.0.1",
        "gatsby": "^5.7.0",
        "gatsby-plugin-google-analytics": "^5.7.0",
        "gatsby-plugin-image": "^3.7.0",
        "gatsby-plugin-manifest": "^5.7.0",
        "gatsby-plugin-offline": "^6.7.0",
        "gatsby-plugin-postcss": "^6.7.0",
        "gatsby-plugin-sharp": "^5.8.1",
        "gatsby-remark-highlights": "^1.3.4",
        "gatsby-remark-images": "^7.7.0",
        "gatsby-source-filesystem": "^5.7.0",
        "gatsby-transformer-remark": "^6.7.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@gatsbyjs/reach-router": "^2.0.1",
        "@lhci/cli": "^0.11.0",
        "@percy/cli": "^1.20.3",
        "@percy/cypress": "^3.1.2",
        "@testing-library/cypress": "^9.0.0",
        "@testing-library/react": "^14.0.0",
        "@types/gatsbyjs__reach-router": "^2.0.0",
        "@types/jest": "^29.4.0",
        "@types/node": "^18.15.0",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "autoprefixer": "^10.4.14",
        "cypress": "^12.7.0",
        "eslint": "^8.36.0",
        "eslint-config-xo-react": "^0.27.0",
        "eslint-plugin-cypress": "^2.12.1",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jest-environment-jsdom": "^29.5.0",
        "postcss": "^8.4.21",
        "tailwindcss": "^3.2.7",
        "xo": "^0.53.1"
    }
}
