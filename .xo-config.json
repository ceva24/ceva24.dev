{
    "extends": [
        "plugin:prettier/recommended",
        "xo-react",
        "plugin:cypress/recommended"
    ],
    "rules": {
        "react/jsx-indent": "off",
        "react/jsx-indent-props": "off",
        "react/jsx-tag-spacing": "off",
        "react/react-in-jsx-scope": "off"
    },
    "ignores": ["gatsby-*.js", "*.config.js"]
}
