{
    "ci": {
        "collect": {
            "url": ["http://localhost:9000/"],
            "startServerCommand": "npm run serve",
            "startServerReadyPattern": "in the browser"
        },
        "assert": {
            "preset": "lighthouse:recommended"
        },
        "upload": {
            "target": "temporary-public-storage"
        }
    }
}
