{
    "name": "ceva24.dev",
    "private": true,
    "version": "1.0.0",
    "description": "Static website built with TypeScript and Gatsby",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/ceva24/ceva24.dev"
    },
    "keywords": [
        "gatsby",
        "website",
        "tailwindcss"
    ],
    "scripts": {
        "prepare": "husky install",
        "develop": "gatsby develop",
        "start": "gatsby develop",
        "build": "gatsby build",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "test": "jest --coverage",
        "e2e": "cypress run",
        "format": "prettier --write .",
        "check-format": "prettier --check .",
        "lint": "xo",
        "check-types:gatsby": "tsc",
        "check-types:cypress": "tsc --project cypress/tsconfig.json",
        "check-types": "npm run check-types:gatsby && npm run check-types:cypress",
        "check": "npm run check-format && npm run lint && npm run check-types && npm run test",
        "fc": "npm run format && npm run lint && npm run check-types && npm run test"
    },
    "dependencies": {
        "@fortawesome/fontawesome-svg-core": "^1.2.35",
        "@fortawesome/free-brands-svg-icons": "^5.15.3",
        "@fortawesome/free-solid-svg-icons": "^5.15.3",
        "@fortawesome/react-fontawesome": "^0.1.14",
        "gatsby": "^3.8.1",
        "gatsby-plugin-google-analytics": "^3.8.0",
        "gatsby-plugin-image": "^1.8.0",
        "gatsby-plugin-manifest": "^3.8.0",
        "gatsby-plugin-offline": "^4.8.0",
        "gatsby-plugin-postcss": "^4.8.0",
        "gatsby-plugin-react-helmet": "^4.8.0",
        "gatsby-plugin-sharp": "^3.8.0",
        "gatsby-remark-highlights": "^1.3.4",
        "gatsby-remark-images": "^5.5.0",
        "gatsby-source-filesystem": "^3.8.0",
        "gatsby-transformer-remark": "^4.5.0",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0"
    },
    "devDependencies": {
        "@percy/cli": "^1.0.0-beta.58",
        "@percy/cypress": "^3.1.0",
        "@testing-library/cypress": "^7.0.6",
        "@testing-library/react": "^12.0.0",
        "@types/jest": "^26.0.23",
        "@types/react-helmet": "^6.1.1",
        "autoprefixer": "^10.2.6",
        "cypress": "^7.6.0",
        "eslint-config-xo-react": "^0.25.0",
        "eslint-plugin-cypress": "^2.11.3",
        "husky": "^7.0.0",
        "jest": "^27.0.6",
        "postcss": "^8.3.5",
        "tailwindcss": "^2.2.4",
        "xo": "^0.40.3"
    }
}
